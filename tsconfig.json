{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "jest"]
  }
  ,
  "include": [
    "src/**/*",
    "__tests__/**/*",
    "prisma/**/*",
    "Contracts/**/*"
  ],
  "exclude": ["node_modules", "dist"]
}
